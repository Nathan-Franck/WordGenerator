{
	"name": "wordgenerator",
	"displayName": "Word Generator",
	"description": "Help spur some creativity with some random words amongst your existing text!",
	"version": "1.0.0",
	"publisher": "TacticalDan",
	"repository": {
		"url": "https://github.com/TacticalDan/WordGenerator.git"
	},
	"icon": "WordGenLogo.png",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.adjective",
		"onCommand:extension.verb",
		"onCommand:extension.noun"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.adjective",
				"title": "Replace with Random Adjective",
				"category": "Word Generator"
			},
			{
				"command": "extension.noun",
				"title": "Replace with Random Noun",
				"category": "Word Generator"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	}
}
